    "eslint-plugin-react": "^7.7.0",
    "eslint-scope": "^3.7.1",
    "husky": "^4.2.5",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "regenerator-runtime": "^0.11.1",
    "request": "^2.85.0",
    "sepia": "^2.0.2"
